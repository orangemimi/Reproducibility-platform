{
    "behavior": {
        "inputs": [
            {
                "name": "data_for_initial_mining",
                "description": "起始年份土地利用分类数据",
                "isOptional": false
            },
            {
                "name": "conversion_Matrix",
                "isOptional": false,
                "description": "多种土地利用类型之间相互进行转换的可能性矩阵,本矩阵以0,1值决定两个土地利用类型之间是否可以发生转换，1表示可以转换，0表示不能转换。此设定用来控制模拟获取更合理的结果，如城市用地一般不会转换为林地、草地等类型，研究区的林地一般不会转换草地等。"
            }
        ],
        "outputs": [
            {
                "name": "sampling_cells_count",
                "description": "采样总数目",
                "isOptional": false
            }
        ],
        "parameters": [
            {
                "defaultValue": "10",
                "type": "input_parameter",
                "tooltip": "%",
                "description": "对数据进行何种比例的抽样，即抽样数据的记录总数=起始年份土地利用分类数据的栅格总数×抽样百分比。比例越大，抽样数据越大，则训练ANN时间越大，但需有一定的抽样比例以保证数据覆盖性。",
                "name": "sampling_scale ",
                "_class": "edu.njnu.opengms.common.domain.container.model.support.InputParameter"
            },
            {
                "defaultValue": "10-30",
                "type": "range_parameter",
                "from": "0",
                "to": "100",
                "tooltip": "测试Range",
                "description": "测试Range",
                "name": "sampling_scale ",
                "_class": "edu.njnu.opengms.common.domain.container.model.support.RangeParameter"
            },
            {
                "defaultValue": "10",
                "type": "slider_parameter",
                "tooltip": "测试Slider",
                "description": "测试Slider",
                "name": "测试Slider",
                "min": "0",
                "max": "100",
                "step": "10",
                "_class": "edu.njnu.opengms.common.domain.container.model.support.SliderParameter"
            },
            {
                "defaultValue": "2",
                "type": "select_parameter",
                "tooltip": "测试Select",
                "name": "测试Select",
                "keyValue": {
                    "1": "显示内容1",
                    "2": "显示内容2",
                    "3": "显示内容3"
                },
                "description": "测试Select",
                "_class": "edu.njnu.opengms.common.domain.container.model.support.SelectParameter"
            },
            {
                "defaultValue": "水量,水温,水速,含N量\n1,2,3,4\n5,6,7,8",
                "type": "table_parameter",
                "tooltip": "测试Talbe",
                "name": "测试Talbe ",
                "description": "测试Talbe",
                "fields": [
                    "水量",
                    "水温",
                    "水速",
                    "含N量"
                ],
                "_class": "edu.njnu.opengms.common.domain.container.model.support.TableParameter"
            }
        ]
    }
}